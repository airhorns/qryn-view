{
    "plugins": [
        "transform-class-properties",
        "react-hot-loader/babel",
        "syntax-dynamic-import",
        "@babel/plugin-transform-runtime"
    ],
    "presets": [
    
        "@babel/preset-env",
        [
            
            "@babel/preset-react",
            {
                "runtime": "automatic"
                
            }
        ]
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}